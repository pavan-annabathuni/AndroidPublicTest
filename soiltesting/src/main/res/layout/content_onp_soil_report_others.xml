<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@drawable/selector_dashboard_corner_border">
    <TextView
        android:id="@+id/others_tittle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/selector_top_radius_blank"
        android:backgroundTint="@color/light_grey"
        android:padding="@dimen/space16"
        android:textColor="@color/black_two"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="@dimen/space8"
        android:layout_marginBottom="@dimen/space16"
        android:layout_marginLeft="@dimen/space16"
        android:layout_marginRight="@dimen/space16">
        <TextView
            android:id="@+id/soil_report_tv1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/space8"
            android:textColor="@color/black_two"

            />
        <TextView
            android:id="@+id/soil_report_tv2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/space8"
            android:textColor="@color/black_two"

            />
        <TextView
            android:id="@+id/soil_report_tv3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/space8"
            android:textColor="@color/black_two"

/>
        <TextView
            android:id="@+id/soil_report_tv4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/space8"
            android:textColor="@color/black_two"
           />
        <TextView
            android:id="@+id/soil_report_tv5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/space8"
            android:textColor="@color/black_two"
           />
        <TextView
            android:id="@+id/soil_report_tv6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/space8"
            android:textColor="@color/black_two"
            />

    </LinearLayout>
</LinearLayout>