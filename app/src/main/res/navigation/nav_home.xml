<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_home"
    app:startDestination="@id/homePagesFragment">

    <fragment
        android:id="@+id/homePagesFragment"
        android:name="com.waycool.iwap.home.HomePagesFragment"
        android:label="fragment_home_pages"
        tools:layout="@layout/fragment_home_pages">
        <action
            android:id="@+id/action_homePagesFragment_to_newsFullviewActivity2"
            app:destination="@id/newsFullviewActivity2"
            app:popUpTo="@id/homePagesFragment" />
        <action
            android:id="@+id/action_homePagesFragment_to_playVideoFragment4"
            app:destination="@id/playVideoFragment4"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right"
            app:popUpTo="@id/homePagesFragment" />
        <action
            android:id="@+id/action_homePagesFragment_to_allServicesFragment"
            app:destination="@id/allServicesFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right" />
        <action
            android:id="@+id/action_homePagesFragment_to_mandiGraphFragment22"
            app:destination="@id/mandiGraphFragment2"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right" />
        <action
            android:id="@+id/action_homePagesFragment_to_editCropFragment"
            app:destination="@id/editCropFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right"
            app:popUpTo="@id/homePagesFragment" />
        <action
            android:id="@+id/action_homePagesFragment_to_homePagePremiumFragment2"
            app:destination="@id/homePagePremiumFragment2" />
        <action
            android:id="@+id/action_homePagesFragment_to_spotLightFragment"
            app:destination="@id/spotLightFragment" />
        <action
            android:id="@+id/action_homePagesFragment_to_farmDetailsFragment"
            app:destination="@id/farmDetailsFragment"
            app:popUpTo="@id/homePagesFragment" />
        <action
            android:id="@+id/action_homePagesFragment_to_notificationFragment"
            app:destination="@id/notificationFragment" />
    </fragment>
    <activity
        android:id="@+id/newsFullviewActivity2"
        android:name="com.waycool.newsandarticles.view.NewsFullviewActivity"
        android:label="NewsFullviewActivity" />
    <fragment
        android:id="@+id/playVideoFragment4"
        android:name="com.waycool.videos.fragments.PlayVideoFragment"
        android:label="PlayVideoFragment" />

    <fragment
        android:id="@+id/allServicesFragment"
        android:name="com.waycool.iwap.allservices.AllServicesFragment"
        android:label="fragment_all_services"
        tools:layout="@layout/fragment_all_services">
        <action
            android:id="@+id/action_allServicesFragment_to_homePagesFragment"
            app:destination="@id/homePagesFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right"
            app:popUpTo="@id/homePagesFragment"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/mandiGraphFragment2"
        android:name="com.example.mandiprice.fragments.MandiGraphFragment"
        android:label="MandiGraphFragment">
        <action
            android:id="@+id/action_homePagesFragment_to_mandiGraphFragment2"
            app:destination="@id/mandiGraphFragment2" />
    </fragment>
    <fragment
        android:id="@+id/editCropFragment"
        android:name="com.example.addcrop.EditCropFragment"
        android:label="EditCropFragment" />
    <fragment
        android:id="@+id/spotLightFragment"
        android:name="com.waycool.iwap.spotlight.SpotLightFragment"
        android:label="fragment_spot_light"
        tools:layout="@layout/fragment_spot_light">
        <action
            android:id="@+id/action_spotLightFragment_to_homePagesFragment"
            app:destination="@id/homePagesFragment" />
    </fragment>
    <fragment
        android:id="@+id/farmDetailsFragment"
        android:name="com.example.soiltesting.premium.FarmDetailsFragment"
        android:label="FarmDetailsFragment" />
    <fragment
        android:id="@+id/homePagePremiumFragment2"
        android:name="com.waycool.iwap.premium.HomePagePremiumFragment"
        android:label="fragment_home_page_premium"
        tools:layout="@layout/fragment_home_page_premium">
        <action
            android:id="@+id/action_homePagePremiumFragment2_to_farmDetailsFragment2"
            app:destination="@id/farmDetailsFragment2" />
        <action
            android:id="@+id/action_homePagePremiumFragment2_to_deviceFragmentOne"
            app:destination="@id/deviceFragmentOne" />
        <action
            android:id="@+id/action_homePagePremiumFragment2_to_graphsFragment"
            app:destination="@id/graphsFragment" />
        <action
            android:id="@+id/action_homePagePremiumFragment2_to_farmDetailsFragment3"
            app:destination="@id/farmDetailsFragment3" />
    </fragment>
    <fragment
        android:id="@+id/farmDetailsFragment2"
        android:name="com.waycool.iwap.farmdetails.FarmDetailsFragment"
        android:label="fragment_farm_details"
        tools:layout="@layout/fragment_farm_details" />
    <fragment
        android:id="@+id/deviceFragmentOne"
        android:name="com.waycool.iwap.premium.DeviceFragmentOne"
        android:label="fragment_device_one"
        tools:layout="@layout/fragment_device_one" />
    <fragment
        android:id="@+id/graphsFragment"
        android:name="com.waycool.iwap.premium.GraphsFragment"
        android:label="fragment_graphs"
        tools:layout="@layout/fragment_graphs" />
    <fragment
        android:id="@+id/farmDetailsFragment3"
        android:name="com.waycool.iwap.premium.FarmDetailsFragment"
        android:label="fragment_farm_details"
        tools:layout="@layout/fragment_farm_details" />
    <fragment
        android:id="@+id/notificationFragment"
        android:name="com.waycool.iwap.notification.NotificationFragment"
        android:label="fragment_notification"
        tools:layout="@layout/fragment_notification" />

</navigation>